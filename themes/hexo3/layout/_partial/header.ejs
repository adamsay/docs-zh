<header id="header">
  <div class="outer">
    <div class="inner">
      <h1 id="logo-wrap">
        <a href="<%- url_for() %>" id="logo">Hexo</a>
        <a id="main-nav-toggle"></a>
      </h1>
      <nav id="main-nav">
        <ul id="main-nav-list">
          <% _.each(theme[get_menu()], function(menu, title){ %>
            <li class="main-nav-item">
              <a href="<%= url_for(menu) %>" class="main-nav-link"><%= title %></a>
            </li>
          <% }); %>
        </ul>
        <a href="<%= toggle_lang_url() %>" class="main-nav-lang"><%= toggle_lang_title() %></a>
        <a href="https://github.com/<%- theme.github %>" id="github-btn" title="GitHub" target="_blank"></a>
        <a id="search-btn" class="st-search-launcher" title="Search"></a>
      </nav>
    </div>
  </div>
</header>
