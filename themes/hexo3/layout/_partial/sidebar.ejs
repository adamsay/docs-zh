<aside id="sidebar">
  <% _.each(theme[get_sidebar()], function(menu, title){ %>
    <strong class="sidebar-title"><%= title %></strong>
    <% _.each(menu, function(link, text){ %>
      <% var finalLink = (link + "/index.html").replace(/\/{2}/, '/'); %>
      <a href="<%- url_for(link) %>" class="sidebar-link<% if (is_current(finalLink, true)){ %> current<% } %>"><%= text %></a>
    <% }) %>
  <% }) %>
  <a href="http://prose.io/#fireball-x/docs/edit/master/themes/hexo3/_config.yml" id="sidebar-edit">Edit Navigation</a>
</aside>
